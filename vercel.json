{
    "version": 2,
    "builds": [
      {
        "src": "src/next.config.mjs",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/app/api/$1"
      },
      {
        "src": "/cadastrar/(.*)",
        "dest": "/src/app/cadastrar/$1"
      },
      {
        "src": "/carrinho/(.*)",
        "dest": "/src/app/carrinho/$1"
      },
      {
        "src": "/catalogo/(.*)",
        "dest": "/src/app/catalogo/$1"
      },
      {
        "src": "/favoritos/(.*)",
        "dest": "/src/app/favoritos/$1"
      },
      {
        "src": "/footer/(.*)",
        "dest": "/src/app/footer/$1"
      },
      {
        "src": "/header/(.*)",
        "dest": "/src/app/header/$1"
      },
      {
        "src": "/homeArrays/(.*)",
        "dest": "/src/app/homeArrays/$1"
      },
      {
        "src": "/login/(.*)",
        "dest": "/src/app/login/$1"
      },
      {
        "src": "/pedidos/(.*)",
        "dest": "/src/app/pedidos/$1"
      },
      {
        "src": "/perfil/(.*)",
        "dest": "/src/app/perfil/$1"
      },
      {
        "src": "/product/(.*)",
        "dest": "/src/app/product/$1"
      },
      {
        "src": "/productMV/(.*)",
        "dest": "/src/app/productMV/$1"
      },
      {
        "src": "/redefinir-senha/(.*)",
        "dest": "/src/app/redefinir-senha/$1"
      },
      {
        "src": "/sobre/(.*)",
        "dest": "/src/app/sobre/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/app/$1"
      }
    ]
  }
  